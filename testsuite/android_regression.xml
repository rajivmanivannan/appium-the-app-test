<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Suite" parallel="tests">

    <groups>
        <run>
            <include name="Regression"/>
        </run>
    </groups>

    <listeners>
        <listener class-name="com.ronetech.theapptest.utils.TestListener"/>
    </listeners>

    <test thread-count="5" name="Android">
        <classes>
            <class name="com.ronetech.theapptest.RegressionTest"/>
        </classes>
    </test>  <!-- Test -->
</suite>